<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.clf.dao.UserMapper">
 
  <select id="getUser" resultType="com.clf.domain.User" parameterType="java.lang.Integer">
       select aid as id ,relname as name from admin_info where aid=#{id}
  </select>
 
</mapper>